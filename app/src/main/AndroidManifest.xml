<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codewithleo.multiplicationtable">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MultiplicationTable">
        <activity android:name=".ContactME"></activity>
        <activity
            android:name=".TicTacToe"
            android:label="@string/title_activity_tic_tac_toe"
            android:theme="@style/Theme.MultiplicationTable.NoActionBar" />
        <activity android:name=".Alphabets" />
        <activity android:name=".MulTable" />
        <activity android:name=".Dashboard" android:theme="@style/Theme.MultiplicationTable.NoActionBar"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>